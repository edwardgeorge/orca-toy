	( animate ) IO-TYPE STH2r ;data/types ADD2 STA
	( get note ) ;chrmid JSR2 SWP [ #0c MUL ] ADD .Console/write DEO

JMP2r
	&? '; "Send 20 "a 20 "raw 20 "pitch 20 "byte $1

